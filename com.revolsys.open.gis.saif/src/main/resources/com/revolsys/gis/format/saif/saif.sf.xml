<beans>
  <bean id="saifReader" class="com.revolsys.gis.format.saif.io.SaifReader"
    singleton="false">
    <constructor-arg>
      <value>${sourceFile}</value>
    </constructor-arg>
    <property name="convertGeometries" value="true"/>
  </bean>
  <bean id="saifWriter" class="com.revolsys.gis.format.saif.io.SaifWriter"
    singleton="false">
    <constructor-arg>
      <value>${targetFile}</value>
    </constructor-arg>
    <property name="compositeTypeNames">
      <map>
        <entry key="SpatialReferencing" value="SpatialReferencing"/>
      </map>
    </property>
    <property name="indentEnabled" value="true"/>
  </bean>
</beans>