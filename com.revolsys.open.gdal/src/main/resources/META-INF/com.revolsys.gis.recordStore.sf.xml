<?xml version="1.0" encoding="UTF-8"?>
<beans
  xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:util="http://www.springframework.org/schema/util"
  xmlns:p="http://www.springframework.org/schema/p"
  xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
  "
>
  <bean
    class="com.revolsys.data.io.RecordStoreFactoryRegistry"
    factory-method="register"
  >
    <constructor-arg>
      <bean class="com.revolsys.gdal.record.OgrRecordStoreFactory">
        <constructor-arg
          index="0"
          value="SQLite/SpatiaLite" />
        <constructor-arg
          index="1"
          value="application/x-sqlite3" />
        <constructor-arg
          index="2"
        >
          <list>
            <value>sqlite</value>
          </list>
        </constructor-arg>
      </bean>
    </constructor-arg>
  </bean>
  <bean
    class="com.revolsys.data.io.RecordStoreFactoryRegistry"
    factory-method="register"
  >
    <constructor-arg>
      <bean class="com.revolsys.gdal.record.OgrRecordStoreFactory">
        <constructor-arg
          index="0"
          value="GeoPackage" />
        <constructor-arg
          index="1"
          value="application/x-gpkg" />
        <constructor-arg
          index="2"
          name="fileNameExtensions"
        >
          <list>
            <value>gpkg</value>
          </list>
        </constructor-arg>
      </bean>
    </constructor-arg>
  </bean> 
</beans>
