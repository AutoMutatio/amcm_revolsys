<?xml version="1.0" encoding="UTF-8"?>
<Configuration
  strict="true"
>
  <Appenders>
    <Appender
      type="Console"
      name="Console"
    >
      <Layout
        type="PatternLayout"
        pattern="%p\t%c\t%m%n" />
    </Appender>
  </Appenders>

  <Loggers>
    <Logger name="com.revolsys.core.test.gis.esri.gdb.file" level="DEBUG" />
    
    <Root level="WARN">
      <AppenderRef ref="Console" />
    </Root>
  </Loggers>

</Configuration>
